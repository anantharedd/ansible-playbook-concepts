pipeline {
    agent any

    stages {
        stage('SCM') {
            steps {
                git 'https://github.com/anantharedd/ansible-playbook-concepts.git'
            }
        }
        stage('docker build') {
            steps {
                sh 'sudo docker build -t jilu407/akbar-app:$BUILD_NUMBER . '
            }
        }
    }
}

